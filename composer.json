{
    "name": "webcraft/lunar-mollie",
    "type": "project",
    "description": "Mollie payment driver for Lunar.",
    "keywords": [
        "lunarphp",
        "laravel",
        "ecommerce",
        "e-commerce",
        "headless",
        "store",
        "shop",
        "cart",
        "mollie",
        "applepay",
        "bancontact",
        "banktransfer",
        "belfius",
        "creditcard",
        "directdebit",
        "eps",
        "giftcard",
        "giropay",
        "ideal",
        "kbc",
        "mybank",
        "paypal",
        "paysafecard",
        "przelewy24",
        "sofort"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Webcraft",
            "homepage": "https://www.webcraft.be/"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.00",
        "lunarphp/lunar": "^1.0.0-beta.6",
        "livewire/livewire": "^3.0",
        "mollie/laravel-mollie": "^v3.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Webcraft\\Lunar\\Mollie\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webcraft\\Lunar\\Mollie\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "extra": {
        "lunar": {
            "name": "Mollie Payments"
        },
        "laravel": {
            "providers": [
                "Webcraft\\Lunar\\Mollie\\MolliePaymentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
